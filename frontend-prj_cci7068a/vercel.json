{
  "buildCommand": "cd frontend-prj_cci7068a && npm install && npm run build",
  "outputDirectory": "frontend-prj_cci7068a/dist",
  "installCommand": "cd frontend-prj_cci7068a && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  }
}
